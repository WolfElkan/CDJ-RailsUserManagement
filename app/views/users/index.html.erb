<h1>Listing users</h1>
<table>
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</thead>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.first_name %></td>
			<td><%= user.last_name %></td>
			<td><%= user.email_address %></td>
		</tr>
	<% end %>
</table>
<a href="/users/new">New User</a>